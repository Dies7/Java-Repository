package exercises;
import javax.swing.JOptionPane;
class AscendingAndDescending {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=1;
		        String desas;
		        do{
		            JOptionPane.showInputDialog("Enter number: "+num);
		            num++;
		        }while (num<=10); 
		        do{
		            desas=JOptionPane.showInputDialog(null,"What order?(descending/ascending)");
		                }while(desas.equalsIgnoreCase(""));

	}

}
